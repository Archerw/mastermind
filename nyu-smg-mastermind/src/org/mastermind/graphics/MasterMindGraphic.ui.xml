<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <g:VerticalPanel>
    <g:HorizontalPanel width="100%" height="190px">
      <g:cell width="30%">
        <g:VerticalPanel width="100%" ui:field="historyArea"/>
      </g:cell>
      <g:cell width="30%">
        <g:VerticalPanel width="100%" ui:field="feedbackArea"/>
      </g:cell>
      <g:cell width="40%">
        <g:VerticalPanel width="100%" ui:field="infoArea"/>
      </g:cell>
    </g:HorizontalPanel>
    <g:HorizontalPanel height="18px" width="100%" ui:field="inputInfoArea"/>
    <g:HorizontalPanel height="18px" width="100%" ui:field="messageArea"/>
    <g:AbsolutePanel height="90px" width="100%" ui:field="buttonArea" />
    <g:HorizontalPanel height="36px" width="100%" ui:field="dragArea" />
  </g:VerticalPanel>
</ui:UiBinder> 